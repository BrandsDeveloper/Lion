{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.0"
      }
    },
    "routes": [
        { "handle": "filesystem" },
        { "src": "/api/css/", "dest": "/api/css/*.css" },
        { "src": "/api/img/", "dest": "/api/img/$1" },
        { "src": "/api/js/", "dest": "/api/js/*.js" },
        { "src": "/api/src/(.*)", "dest": "/api/src/$1" },
        { "src": "/api/", "dest": "/api/*.php" }
    ]
  }
  