{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    { "src": "/",  "dest": "/api/index.php" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/*.js" },
    { "src": "/img/(.*)", "dest": "/img/$1" },
    { "src": "/api/(.*)", "dest": "/api/*.php" }
  ]
}
