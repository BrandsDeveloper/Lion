{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.0"
      }
    },
    "routes": [
      { "src": "/css/(.*)\\.css$", "dest": "/api/css/$1.css" },
      { "src": "/js/(.*)\\.js$", "dest": "/api/js/$1.js" },
      { "src": "/img/(.*)\\.png$", "dest": "/api/img/$1.png" },
      { "src": "/src/(.*)\\.php$", "dest": "/api/src/$1.php" }
    ]
  }
  