{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.0"
      }
    },
    "routes": [
        { "src": "/api/css/(.*)", "methods": ["GET"], "dest": "/api/css/*.css" },
        { "src": "/api/js/(.*)", "methods": ["GET"], "dest": "/api/js/*.js" },
        { "src": "/api/img/(.*)", "methods": ["GET"], "dest": "/api/img/*.png" },
        { "src": "/api/src/(.*)", "methods": ["GET"], "dest": "/api/src/*.scss" }
    ]
  }
  